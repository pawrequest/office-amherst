[Version]
signature="$WINDOWS NT$"
Class=Modem
ClassGuid={4D36E96D-E325-11CE-BFC1-08002BE10318}
Provider=%Provider%
DriverVer=09/08/2017,5.38.10.0
CatalogFile=bkrwmdm.cat

[ControlFlags]
ExcludeFromSelect = *

[SourceDisksNames.x86]
; diskid = description[, [tagfile] [, <unused>, subdir]]
1=%DiskString%,"bkrwmdm.sys",,\i386

[SourceDisksNames.amd64]
; diskid = description[, [tagfile] [, <unused>, subdir]]
1=%DiskString%,"bkrwmdm.sys",,\amd64


[SourceDisksFiles]
; filename = diskid[,[ subdir][, size]]
bkrwmdm.sys = 1
bkrwcmnt.sys = 1
bkrwmdfl.sys = 1

;-----------------------------------------------------------------------------
; Specify target paths...
;-----------------------------------------------------------------------------

[DestinationDirs]
DefaultDestDir = 12                    ; system32\drivers

;-----------------------------------------------------------------------------
; Table of Contents
;-----------------------------------------------------------------------------
[Manufacturer]
%Digital%=Digital, NTamd64

;-----------------------------------------------------------------------------
; Device-by-device entries
;-----------------------------------------------------------------------------
[Digital]
%Digital.Modem.Desc% = Digital.Install, USB\VID_238b&PID_0a11&Class_02&SubClass_02&Prot_01&OS_NT
%Digital.Modem.Desc% = Digital.Install, USB\VID_238b&PID_0a12&Class_02&SubClass_02&Prot_01&OS_NT
