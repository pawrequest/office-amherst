Digital.ndi5.guid = "BKRWCR\{23BC93D8-82CA-443E-A17D-027BDEE867D1}"
Digital.ndi6.guid = "BKRWCR\{C204E6DF-D615-4E6F-91D3-8B44F4BBD739}"
Digital.ndi62.guid = "BKRWCR\{4E33C73D-7923-47E3-8266-552145E58793}"


