[Digital.NTamd64]
%Digital.Modem.Desc% = Digital.Install, USB\VID_238b&PID_0a11&Class_02&SubClass_02&Prot_01&OS_NT
%Digital.Modem.Desc% = Digital.Install, USB\VID_238b&PID_0a12&Class_02&SubClass_02&Prot_01&OS_NT
