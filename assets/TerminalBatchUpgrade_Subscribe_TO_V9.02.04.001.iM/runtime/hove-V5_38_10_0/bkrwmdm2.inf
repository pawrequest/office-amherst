%Digital.Modem.Desc% = Digital.Install, USB\VID_238b&PID_0a51&Class_02&SubClass_02&Prot_01&OS_NT
%Digital.Modem.Desc% = Digital.Install, USB\VID_238b&PID_0a52&Class_02&SubClass_02&Prot_01&OS_NT



;-----------------------------------------------------------------------------
; Installation instructions
;-----------------------------------------------------------------------------

[Digital.Install.nt]
AddReg = Digital.AddReg, Digital.NT.AddReg, GenericAddReg, USB
CopyFiles	= CopyFiles, CopyFiles.Win2K


;-----------------------------------------------------------------------------
;  Registry
;-----------------------------------------------------------------------------

; common registry entries 
[Digital.AddReg]
HKR,,EnableSetClearFeatureIdle, 0x10001, 01,00,00,00
HKR,,OverrideDefaultPortSettings, 0x10001, 01,00,00,00
HKR,,InitialBaudRate, 0x10001, 00,C2,01,00			;115200 initial baud rate
HKR,,InitialLineControl,, "8N1"					;8-bits, No parity, 1 stop bit
HKR,,EnableSetClearFeatureIdle, 0x10001, 01,00,00,00

; only for NT...
[Digital.NT.AddReg]
HKR,,Properties,  1, 00,00,00,00, FF,00,00,00, 00,00,00,00, 00,00,00,00, 00,00,00,00, 31,00,00,00, 00,08,07,00, 00,DC,05,00
HKR, Responses, "<cr><lf>CONNECT<cr><lf>", 1, 02, 00, 00,DC,05,00, 00,00,00,00
HKR,,MaximumPortSpeed, 1, 00,DC,05,00
HKR, Hangup, 1,, "ATH<cr>"
HKR,,EnableSelectiveSuspend, 0x10001, 00, 00, 00, 00
HKR,,EnableCR3840, 0x10001, 01,00,00,00
HKR,,AllowRemoteWakeup, 0x10001, 01,00,00,00

;-----------------------------------------------------------------------------
;  Copy lists
;-----------------------------------------------------------------------------
[CopyFiles]

[CopyFiles.Win2K]
bkrwmdm.sys
bkrwcm.sys,bkrwcmnt.sys
bkrwcmnt.sys
bkrwmdfl.sys



;-----------------------------------------------------------------------------
;  NT Services installation
;-----------------------------------------------------------------------------
[Digital.Install.NT.HW]
AddReg = LowerFilterAddReg
AddReg = UpperFilterAddReg



[Digital.Install.NT.Services]
AddService = bkrwmdm, 0x00000000, Digital.Install.AddService, DigitalWdm.Install.EventLog
AddService = bkrwmdfl,, Digital.Install.AddFilter



[UpperFilterAddReg]
HKR,,"UpperFilters",0x00010000,"bkrwmdfl"

[Digital.Install.AddService]
DisplayName	= %Digital.Service.Name%
Description	= %Digital.Service.Desc%
ServiceType	= 1			;%SERVICE_KERNEL_DRIVER%
StartType	= 3			;%SERVICE_DEMAND_START%
ErrorControl	= 1			;%SERVICE_ERROR_NORMAL%	
ServiceBinary	= %12%\bkrwmdm.sys

[Digital.Install.AddFilter]
DisplayName	= %Digital.Filter.Name%
Description	= %Digital.Filter.Desc%
ServiceType	= 1			;%SERVICE_KERNEL_DRIVER%
StartType	= 3			;%SERVICE_DEMAND_START%
ErrorControl	= 1			;%SERVICE_ERROR_NORMAL%	
ServiceBinary	= %12%\bkrwmdfl.sys

;-----------------------------------------------------------------------------
;       Event logging support
;-----------------------------------------------------------------------------

[DigitalWdm.Install.EventLog]
Addreg = DigitalWdm.EventLog.AddReg

[DigitalWdm.EventLog.AddReg]
HKR,,EventMessageFile, 0x20000, "%%SystemRoot%%\System32\drivers\bkrwmdm.sys"
HKR,,TypesSupported, 0x10001, 7

;-----------------------------------------------------------------------------
[LowerFilterAddReg]
HKR,,"LowerFilters",0x00010000,"bkrwmdm"

[GenericAddReg]
HKR,, FriendlyDriver,,unimodem.vxd
HKR,, PortSubClass,1, 02
HKR,, ConfigDialog,, modemui.dll
HKR,, EnumPropPages,, "modemui.dll,EnumPropPages"
HKR,, FClass, 1, c3,00,00,00
;HKR,, VoiceProfile,    1,23,22,09,00
HKR, Fax, CL1FCS,, "2"

HKR, Init, 1,, "AT<cr>"
HKR, Init, 2,, "AT&F<cr>"
HKR, Init, 3,, "ATV1E0S0=0&D2&C1<cr>"
HKR, Init, 4,, "AT+CMEE=1<cr>"
HKR, Monitor, 1,, "ATS0=0<cr>"
HKR, Monitor, 2,, "None"
HKR, Answer, 1,, "ATA<cr>"
HKR,, Reset,, "AT&F<cr>"
HKR, Settings, FlowControl_Hard,, "+IFC=2,2;"
HKR, Settings, FlowControl_Off,,  "+IFC=0,0;"
HKR, Settings, FlowControl_Soft,, "+IFC=1,1;"
HKR, Settings, Compression_Off,, "+DS=0;"
HKR, Settings, Compression_On,, "+DS=3,0,2048,32;"   
HKR, Settings, CompatibilityFlags, 1, 01 ,00 ,00, 00
HKR, Settings, CallSetupFailTimer,, "S7=<#>;"
HKR, Settings, DialPrefix,, "D"
HKR, Settings, DialSuffix,, ";"
HKR, Settings, Prefix,, "AT"
HKR, Settings, Terminator,, "<cr>"
HKR, Protocol\GSM\ANALOG_RLP,1,, "AT<cr>"


